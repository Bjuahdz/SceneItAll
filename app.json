{
  "expo": {
    "name": "SceneItAll",
    "slug": "SceneItAll",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logov2.png",
    "scheme": "movies",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logov2.png",
        "backgroundColor": "#000000"
      },
      "permissions": [],
      "package": "com.anonymous.SceneItAll"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/logov2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/logov2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0d100e"
        }
      ],
      "react-native-video",
      "expo-video",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
